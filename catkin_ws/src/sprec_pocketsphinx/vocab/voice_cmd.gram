#JSGF V1.0;

/**
 * JSGF Grammar similar to kws example
 */

grammar voice_cmd;

public <move> = MOVE FORWARD;

public <move2> = ROBOT <verb> [<opc1>] <object> <mandatory> [<opc1>] <loc> | MOVE [<opc1>] <object> <mandatory> [<opc1>] <loc>;

<verb> = MOVE;

<opc1> = THE;

<object> = DRINK | PRINGLES;

<mandatory> = TO;

<loc> = TABLE | KITCHEN;
